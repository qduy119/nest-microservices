  "scripts": {
    "build:api-gateway": "nest build api-gateway",
    "build:auth": "nest build auth",
    "build:cart": "nest build cart",
    "build:item": "nest build item",
    "build:notification": "nest build notification",
    "build:elasticsearch": "nest build elasticsearch",
    "build:order": "nest build order",
    "build:payment": "nest build payment",
    "build:user": "nest build user",
    "build:all": "concurrently \"npm run build:api-gateway\" \"npm run build:auth\" \"npm run build:user\" \"npm run build:cart\" \"npm run build:item\" \"npm run build:notification\" \"npm run build:elasticsearch\" \"npm run build:order\" \"npm run build:payment\"",
    "build:shared": "nest build shared",
    "build:utils": "nest build utils",
    "build:libs": "concurrently \"npm run build:shared\" \"npm run build:utils\"",
    "start:api-gateway": "nest start api-gateway",
    "start:auth": "nest start auth",
    "start:cart": "nest start cart",
    "start:item": "nest start item",
    "start:notification": "nest start notification",
    "start:elasticsearch": "nest start elasticsearch",
    "start:order": "nest start order",
    "start:payment": "nest start payment",
    "start:user": "nest start user",
    "start:all": "concurrently \"npm run start:api-gateway\" \"npm run start:auth\" \"npm run start:user\" \"npm run start:cart\" \"npm run start:item\" \"npm run start:notification\" \"npm run start:elasticsearch\" \"npm run start:order\" \"npm run start:payment\"",
    "start:shared": "nest start shared",
    "start:utils": "nest start utils",
    "start:libs": "concurrently \"npm run start:shared\" \"npm run start:utils\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api-gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-gateway/test/jest-e2e.json"
  },
